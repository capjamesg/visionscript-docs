---
title: VisionScript Documentation
layout: default.njk
breadcrumbs: true
---

<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
    {
      "@type": "ListItem",
      "position": 1,
      "name": "VisionScript Documentation",
      "item": "https://visionscript.dev/docs/"
    },
    {
      "@type": "ListItem",
      "position": 2,
      "name": "{{ title }}",
      "item": "{{ url }}"
    }
  ]
}
</script>

<p class="p-summary">{{ definition }}</p>
<div class="e-content">
{{ content | safe }}
</div>
<h2>See Also</h2>
<ul>
    {% for p in collections.std %}
        {% if p.data.tags[1] == tags[1] %}
            <li><a href="{{ p.url }}">{{ p.data.title }}</a></li>
        {% endif %}
    {% endfor %}
</ul>